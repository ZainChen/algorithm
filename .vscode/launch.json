{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "C++ debug (GDB)",
            "type": "cppdbg",
            "request": "launch",
            "targetArchitecture": "x64",
            "program": "${file}.exe",
            "miDebuggerPath": "D:\\Install\\mingw64\\bin\\gdb.exe",
            "args": [
                "zain",
                "jane",
                "ZainJane"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "externalConsole": true,
            "preLaunchTask": "g++"
        },
        {
            "name": "TypeScript",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/ts-node/dist/bin.js",
            "args": [
                "${relativeFile}"
            ],
            "cwd": "${workspaceRoot}",
            // "protocol": "inspector"
        },
        {
            "name": "JavaScript",
            "type": "node",
            "request": "launch",
            "program": "${file}",
            "sourceMaps": true
        },
        {
            "name": "Chrome debug (html)",
            "type": "chrome",
            "request": "launch",
            "file": "${file}",
            "runtimeExecutable": "C:\\Program Files (x86)\\Google\\Chrome\\Application\\chrome.exe",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}",
            "userDataDir": "${tmpdir}",
            "port": 5433
        }
    ]
}